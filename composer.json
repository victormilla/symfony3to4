{
  "name": "symfony/skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A minimal Symfony project recommended to create bare bones applications",
  "repositories": [
    {
      "type": "path",
      "url": "../ark/src/DataCacher"
    },
    {
      "type": "path",
      "url": "../ark/src/DataInputSheets"
    },
    {
      "type": "path",
      "url": "../ark/src/Random"
    },
    {
      "type": "path",
      "url": "../csvJose/csv-objects"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "aircury/collection": "dev-master",
    "arkschools/data-input-sheets": "dev-master",
    "arkschools/random": "dev-master",
    "arkschools/systems-data-cacher": "dev-master",
    "composer/package-versions-deprecated": "1.11.99.2",
    "doctrine/doctrine-bundle": "^2.3",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "friendsofsymfony/rest-bundle": "^3.0",
    "guzzlehttp/guzzle": "^7.3",
    "incenteev/composer-parameter-handler": "^2.1",
    "jd/cron": "dev-master",
    "jd/csv-objects": "dev-master",
    "jms/serializer-bundle": "^3.9",
    "knplabs/knp-snappy-bundle": "^1.8",
    "laminas/laminas-soap": "^2.9",
    "ocramius/proxy-manager": "^2.8",
    "php-amqplib/rabbitmq-bundle": "^2.7",
    "predis/predis": "^1.1",
    "snc/redis-bundle": "^3.4",
    "symfony/console": "4.4.*",
    "symfony/dotenv": "*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "4.4.*",
    "symfony/monolog-bundle": "^3.7",
    "symfony/polyfill-apcu": "^1.22",
    "symfony/proxy-manager-bridge": "4.4.*",
    "symfony/swiftmailer-bundle": "^3.5",
    "symfony/yaml": "4.4.*",
    "twig/twig": "^3.3"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-extension": "^2.3",
    "behat/symfony2-extension": "^2.1",
    "jarnaiz/behat-junit-formatter": "^1.3",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
